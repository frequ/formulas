<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.formula.book"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        <activity
            android:label="@string/app_name"
            android:name="com.formula.book.AndroidDashboardDesignActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <!-- Algebra Activity -->
        <activity android:name="com.formula.book.AlgebraActivity" ></activity>
        
        <!-- Trigonometry Activity -->
        <activity android:name="com.formula.book.TrigonometryActivity" ></activity>
        
        <!-- Geometry Activity -->
        <activity android:name="com.formula.book.GeometryActivity" ></activity>
        
        <!-- Vectors Activity -->
        <activity android:name="com.formula.book.VectorsActivity" ></activity>
        
        <!-- Integrals Activity -->
        <activity android:name="com.formula.book.IntegralsActivity" ></activity>
        
        <!-- Derivative Activity -->
        <activity android:name="com.formula.book.DerivativeActivity" ></activity>
 
        
        
    </application>

</manifest>